            <!--{if $goods.promote_price neq "" } -->
        活动价：<span>￥</span><strong>{$goods.promote_price}</strong>
            <!--{else}-->
        本店价：<span>￥</span><strong>
                {$goods.shop_price}
        </strong>
            <!--{/if}-->
                </li>
				<li class="buy"><a href="javascript:void(0)" class="la" onclick="javascript:collect({$goods.goods_id});">加入收藏</a><a href="javascript:void(0)" class="lb" onclick="javascript:addToCart({$goods.goods_id});">放入购物车</a></li>
			</ul>
<!-- {/if} -->
